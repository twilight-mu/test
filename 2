using namespace std;
#include<iostream>
#include<cstring>
int main()
{
	char a[1000],b[1000],c[1001];
	int n,i,j=1,p=0,len1,len2;
	cin>>n;
	while(n)
	{
		cin>>a>>b;
		len1=strlen(a)-1;len2=strlen(b)-1;
		for(i=0;len1>=0||len2>=0;i++,len1--,len2--)
		{
			if(len1>=0&&len2>=0){ c[i]=a[len1]+b[len2]-'0'+p;}
			if(len1>=0&&len2<0){ c[i]=a[len1]+p;}
			if(len1<0&&len2>=0){ c[i]=b[len2]+p;}
			p=0;
			if(c[i]>'9'){ c[i]=c[i]-10;p=1;}
		}
		cout<<"Case "<<j<<":"<<endl;
		cout<<a<<" + "<<b<<" = ";
		if(p==1)
			cout<<p; 
		while(i--)
			cout<<c[i];
		if(n!=1)
			cout<<endl<<endl;
		else 
			cout<<endl;
		j++;
		n--;
	}
}
